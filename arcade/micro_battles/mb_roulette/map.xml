<spawns>
    <spawn team="blue-team" kit="spawn-kit">
        <regions>
            <point yaw="-45">-15.5,7,-15.5</point> <!-- Blue team should be in NW quadrant -->
        </regions>
    </spawn>
    <spawn team="green-team" kit="spawn-kit">
        <regions>
            <point yaw="45">16.5,7,-15.5</point> <!-- Green team should be in NE quadrant -->
        </regions>
    </spawn>
    <spawn team="red-team" kit="spawn-kit"> 
        <regions>
            <point yaw="135">16.5,7,16.5</point> <!-- Red team should be in SE quadrant -->
        </regions>
    </spawn>
    <spawn team="yellow-team" kit="spawn-kit">
        <regions>
            <point yaw="225">-15.5,7,16.5</point> <!-- Yellow team should be in SW quadrant -->
        </regions>
    </spawn>
    <default>
        <regions>
            <point yaw="0">0.5,38,0.5</point> <!-- Obs spawn, should be modified as needed -->
        </regions>
    </default>
